{
  "name": "asm-date-time",
  "version": "0.0.3",
  "description": "A react component that shows date/time for alya-smart-mirror",
  "engines": {
    "node": ">=6.0.0"
  },
  "main": "public/electron.js",
  "module": "es/component/index.js",
  "files": [
    "es",
    "lib"
  ],
  "scripts": {
    "build": "nwb build-react-component --copy-files",
    "clean": "nwb clean-module",
    "test": "react-scripts test --env=jsdom",
    "electron-start": "react-scripts start",
    "dev": "concurrently \"cross-env BROWSER=none yarn electron-start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "dependencies": {
    "moment": "^2.20.1",
    "electron-is-dev": "^0.3.0",
    "electron-log": "^2.2.13",
    "electron-updater": "^2.17.8",
    "node-notifier": "^5.1.2"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "nwb": "0.20.x",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.3",
    "electron": "^1.7.10",
    "electron-builder": "^19.49.4",
    "react-scripts": "1.0.17",
    "wait-on": "^2.0.2"
  },
  "author": "",
  "homepage": "",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "react-component",
    "alya-smart-mirror"
  ]
}
